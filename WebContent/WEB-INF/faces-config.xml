<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd"
	version="2.1">
	<application>
		<locale-config>
			<supported-locale>en_US</supported-locale>
			<supported-locale>fr_FR</supported-locale>
		</locale-config>
		<resource-bundle>
			<base-name>resources.strings</base-name>
			<var>strings</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>resources.messages</base-name>
			<var>messages</var>
		</resource-bundle>
		<message-bundle>resources.messages</message-bundle>
	</application>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>welcome</from-outcome>
			<to-view-id>/front/welcome.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	      <from-outcome>login</from-outcome>
	      <to-view-id>/front/login.xhtml</to-view-id>
	    </navigation-case>
 	</navigation-rule>
 	
   <navigation-rule>
	    <from-view-id>*</from-view-id>
	    <navigation-case>
	      <from-outcome>inscription</from-outcome>
	      <to-view-id>/front/inscription.xhtml</to-view-id>
	    </navigation-case>
 	</navigation-rule>
 	
 	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>showProfile</from-outcome>
			<to-view-id>/front/showProfile.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>panier</from-outcome>
			<to-view-id>/front/panier.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	
		<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>recap</from-outcome>
			<to-view-id>/front/recapitulatif.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>mesCommandes</from-outcome>
			<to-view-id>/front/mesCommandes.xhtml</to-view-id>
		</navigation-case>
	</navigation-rule>
 	
</faces-config>